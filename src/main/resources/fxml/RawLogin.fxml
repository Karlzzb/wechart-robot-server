<?xml version="1.0" encoding="UTF-8"?>

<?language javascript?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="200.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.karl.fx.controller.LoginController">
	<children>
      <VBox prefHeight="200.0" prefWidth="200.0">
         <children>
      		<ImageView fx:id="loginImageView" fitHeight="171.0" fitWidth="190.0" pickOnBounds="true" preserveRatio="true">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </ImageView>
      		<ProgressBar fx:id="taskBar" prefHeight="18.0" prefWidth="176.0" progress="0.0">
               <VBox.margin>
                  <Insets left="12.0" />
               </VBox.margin>
            </ProgressBar>
         </children>
      </VBox>
	</children>
</AnchorPane>
