<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="consoleTab" prefHeight="600.0" prefWidth="864.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.karl.fx.controller.ConsoleController">
	<children>
      <BorderPane prefHeight="600.0" prefWidth="864.0" styleClass="text-field">
         <center>
            <VBox prefHeight="200.0" prefWidth="100.0" stylesheets="@../css/ghostBlueTheme.css" BorderPane.alignment="CENTER">
               <children>
            		<TextArea fx:id="logArea" prefHeight="562.0" prefWidth="864.0">
                     <VBox.margin>
                        <Insets bottom="30.0" />
                     </VBox.margin>
                  </TextArea>
            		<Button fx:id="cleanButton" mnemonicParsing="false" onAction="#clearLog" prefHeight="100.0" prefWidth="101.0" styleClass="btnBlock" text="清除日志">
		</Button>
               </children>
               <BorderPane.margin>
                  <Insets bottom="10.0" left="10.0" right="50.0" top="30.0" />
               </BorderPane.margin>
            </VBox>
         </center>
      </BorderPane>
	</children>
	<stylesheets>
		<URL value="@../css/Metro-UI.css" />
	</stylesheets>
</AnchorPane>
